{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/worker/create",
      "destination": "/api/worker/create.js"
    },
    {
      "source": "/api/worker/notifications",
      "destination": "/api/worker/notifications.js"
    },
    {
      "source": "/api/webhooks/stripe",
      "destination": "/api/webhooks/stripe.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "headers": [
    {
      "source": "/api/worker/create",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, private"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/api/worker/notifications",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, private"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/api/webhooks/stripe",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, private"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "_docs/**, pages/**"
    },
    "api/worker/create.js": {
      "maxDuration": 300
    },
    "api/worker/notifications.js": {
      "maxDuration": 300
    },
    "api/webhooks/stripe.js": {
      "maxDuration": 30
    }
  }
}